<h1>Vets</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Degree</th>
    <th>Years</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @vets.each do |vet| %>
  <tr>
    <td><%= vet.name %></td>
    <td><%= vet.address %></td>
    <td><%= vet.city %></td>
    <td><%= vet.state %></td>
    <td><%= vet.zip %></td>
    <td><%= vet.degree %></td>
    <td><%= vet.years %></td>
    <td><%= link_to 'Show', vet %></td>
    <td><%= link_to 'Edit', edit_vet_path(vet) %></td>
    <td><%= link_to 'Destroy', vet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Vet', new_vet_path %><br />
<%= link_to 'Appointments', appointments_path %>

